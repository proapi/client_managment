<%= content_tag :div, class: 'table' do %>
    <table class="listing jquery_datatable" cellpadding="0" cellspacing="0">
      <thead>
      <%= content_tag :tr do %>
          <%= content_tag :th, 'Nazwa' %>
          <%= content_tag :th, 'Akcje' %>
      <% end %>
      </thead>
      <tbody>
      <%= content_tag_for(:tr, @agents) do |agent| %>
          <%= content_tag :td, (link_to agent.name, agent), class: 'style1' %>
          <%= content_tag :td do %>
              <%= link_to image_tag('info.png'), agent %>
              <%= link_to image_tag('modify.png'), edit_agent_path(agent) %>
              <%= link_to image_tag('delete.png'), agent, :confirm => "Czy jesteÅ› pewien?", :method => :delete %>
          <% end %>
      <% end %>
      </tbody>
    </table>
<% end %>